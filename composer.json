{
    "name": "artisanry/middlewares",
    "description": "Middlewares for 3rd-Party Packages for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Middlewares", "Laravel Middlewares"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Middlewares\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Middlewares\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
